    {
      "id": "10",
      "text": "Example Search Queries",
      "metadata": {
        "created": "2025-11-05T10:00:00Z",
        "modified": "2025-11-05T10:00:00Z",
        "attributes": {
          "type": "documentation"
        }
      },
      "children": [
        {
          "id": "10-1",
          "text": "/^TODO/ - Find all items starting with TODO",
          "metadata": {
            "created": "2025-11-05T10:01:00Z",
            "modified": "2025-11-05T10:01:00Z"
          },
          "children": []
        },
        {
          "id": "10-2",
          "text": "/^DONE/ - Find all completed items starting with DONE",
          "metadata": {
            "created": "2025-11-05T10:02:00Z",
            "modified": "2025-11-05T10:02:00Z"
          },
          "children": []
        },
        {
          "id": "10-3",
          "text": "/\\d{4}-\\d{2}-\\d{2}/ - Find items with dates",
          "metadata": {
            "created": "2025-11-05T10:03:00Z",
            "modified": "2025-11-05T10:03:00Z"
          },
          "children": []
        },
        {
          "id": "10-4",
          "text": "/@[a-z]+/ - Find items mentioning usernames",
          "metadata": {
            "created": "2025-11-05T10:04:00Z",
            "modified": "2025-11-05T10:04:00Z"
          },
          "children": []
        },
        {
          "id": "10-5",
          "text": "-/^SKIP/ - Find items NOT starting with SKIP",
          "metadata": {
            "created": "2025-11-05T10:05:00Z",
            "modified": "2025-11-05T10:05:00Z"
          },
          "children": []
        },
        {
          "id": "10-6",
          "text": "/bug|issue|problem/ @type=bug - Find bug-related items",
          "metadata": {
            "created": "2025-11-05T10:06:00Z",
            "modified": "2025-11-05T10:06:00Z"
          },
          "children": []
        },
        {
          "id": "10-7",
          "text": "/(?i)todo/ - Case-insensitive search for TODO, todo, Todo",
          "metadata": {
            "created": "2025-11-05T10:07:00Z",
            "modified": "2025-11-05T10:07:00Z"
          },
          "children": []
        },
        {
          "id": "10-8",
          "text": "/^TODO/ d:0 - Root-level TODO items only",
          "metadata": {
            "created": "2025-11-05T10:08:00Z",
            "modified": "2025-11-05T10:08:00Z"
          },
          "children": []
        }
      ]
